varnishtest "Test example vmod"

server s1 {
       rxreq
       txresp
} -start

varnish v1 -vcl+backend {
	import ${vmod_example};

	sub vcl_deliver {
		set resp.http.hello = example.hello("World");
	}
} -start

client c1 {
	txreq -url "/"
	rxresp
	expect resp.http.hello == "Hello, World"
}

client c1 -run
